{
    "manifest_version": 3,
    "name": "News Article Highlighter",
    "version": "1.0",
    "description": "Highlights random sentences in news articles",
    "permissions": ["activeTab"],
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["content.js"]
      }
    ]
  }
  